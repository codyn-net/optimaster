<?xml version="1.0" encoding="UTF-8" ?>

<!-- Job specification -->
<job name="psotest">
	<!-- Type of optimizer to use -->
	<optimizer name="pso">
		<setting name="populationSize">30</setting>
		<setting name="maxIterations">50</setting>
	</optimizer>

	<!-- Optimization parameter boundaries -->
	<boundaries>
		<boundary name="x" min="-100" max="100" />
		<boundary name="y" min="-100" max="100" />
	</boundaries>
	
	<dispatcher name="psotest">
		<setting name="min">-100</setting>
		<setting name="max">100</setting>
	</dispatcher>
</job>
