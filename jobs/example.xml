<?xml version="1.0" encoding="UTF-8" ?>

<!-- Job specification -->
<job name="pso1">
	<!-- Type of optimizer to use -->
	<optimizer name="pso">
		<setting name="populationSize">100</setting>
		<setting name="maxIterations">400</setting>
	</optimizer>

	<!-- Fitness function definition -->
	<fitness name="weighted">
		<setting name="weights">0.6, 0.4</setting>
	</fitness>

	<!-- Optimization parameter boundaries -->
	<boundaries>
		<boundary name="x" min="0" max="100" />
		<boundary name="y" min="0" max="100" />
	</boundaries>
	
	<dispatcher name="webots">
		<setting name="world">myrobot.wbt</setting>
	</dispatcher>
</job>
